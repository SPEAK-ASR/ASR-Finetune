# HuggingFace Configuration
# Copy this file to .env and fill in your values

# Your HuggingFace access token (get from https://huggingface.co/settings/tokens)
HF_TOKEN=hf_your_token_here

# HuggingFace repository ID (format: username/repo-name or org-name/repo-name)
HF_REPO_ID=

# Dataset Directory Configuration (for load_and_push.py)
DATASET_DIR=

# Dataset Creation Configuration (for create-dataset.py)
CSV_FILE=
AUDIO_SOURCE_DIR=
OUTPUT_DIR=
